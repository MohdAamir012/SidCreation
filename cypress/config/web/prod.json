{
  "baseUrl":"https://tataplaybinge.com/",  
  "specPattern" :"cypress/e2e/Features/WEB/*.feature",
    "excludeSpecPattern":["*.ts","*.md"],
    "defaultCommandTimeout": 10000,
    "screenshotsFolder": "artifacts/web/screenshots",
  "videosFolder": "artifacts/web/videos",
  "reporterOptions": {
    "reportDir": "cypress/reports/web/report",
    "charts": true,
    "reportPageTitle": "Binge UI Testing",
    "videoOnFailOnly":true,
    "saveAllAttempts":false,
    "embeddedScreenshots": true,
    "inlineAssets": true
  }
}